{
    "manifestVersion": 1,
    "id": "danhellem-my-first-extension",
    "publisher": "DanHellem",
    "version": "1.0.11",
    "name": "My First Extension",
    "description": "A sample Visual Studio Services extension",
    "public": false,
    "categories": [
        "Azure Boards"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "contributions": [
        {
            "id": "work-item-form-control",
            "type": "ms.vss-work-web.work-item-form-control",
            "targets": [                              
                "ms.vss-work-web.work-item-form"
            ],
            "properties": {
                "name": "Custom control",               
                "uri": "work-item-form-control.html",                 
                "icon": "images/apple.ico",   
                "groupId": "actions", 
                "height": 90
            }
        }
    ],
    "files": [
        {
            "path": "work-item-form-control.html",
            "addressable": true
        },
        {
            "path": "images/apple.ico",
            "addressable": true
        },
        {
            "path": "node_modules/azure-devops-extension-sdk",
            "addressable": true,
            "packagePath": "lib"
        }
    ]
}