{
    "manifestVersion": 1,
    "id": "danhellem-my-first-extension",
    "publisher": "DanHellem2",
    "version": "1.0.18",
    "name": "My First Extension",
    "description": "A sample Visual Studio Services extension",
    "public": false,
    "categories": [
        "Azure Boards"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "contributions": [
        {
            "id": "work-item-form-observer",
            "type": "ms.vss-work-web.work-item-notifications",
            "description": "Require parent links to certain work items",
            "targets": [
                "ms.vss-work-web.work-item-form"
            ],
            "properties": {
                "uri": "parent-link-example.html"
            }
        }
    ],
    "files": [
        {
            "path": "parent-link-example.html",
            "addressable": true
        },
        {
            "path": "images/apple.ico",
            "addressable": true
        },
        {
            "path": "node_modules/azure-devops-extension-sdk",
            "addressable": true,
            "packagePath": "lib"
        }
    ]
}